   "source": [
    "import math\n",
    "product = 1.0\n",
    "for i in range(2,100):\n",
    "    k = 0\n",
    "    f = i / 2\n",
    "    ii = 2\n",
    "    while f >= ii:\n",
    "        if (i % ii) == 0:\n",
    "            k = 1\n",
    "            break\n",
    "        ii += 1\n",
    "    if (k == 0):\n",
    "        product *= 1- 1.0/(i*i)\n",
    "ppp = math.sqrt(6.0/product)\n",
    "print('pi = ' + str(ppp))"
   ]
  }

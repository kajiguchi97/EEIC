    "import sympy\n",
    "sympy.init_printing()\n",
    "x = sympy.Symbol('x')\n",
    "\n",
    "def hermite(n, x):\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    elif n == 1:\n",
    "        return 2*x\n",
    "    else:\n",
    "        return 2*x*hermite(n-1,x) - 2*(n-1)*hermite(n-2,x)\n",
    "    \n",
    "p = hermite(4, x).simplify()\n",
    "p"
   ]
